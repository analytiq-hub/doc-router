{
  "name": "@docrouter/sdk",
  "version": "1.0.0",
  "description": "TypeScript SDK for DocRouter API",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "jest",
    "test:unit": "TEST_TYPE=unit jest tests/unit src/__tests__",
    "test:integration": "TEST_TYPE=integration jest tests/integration",
    "test:all": "npm run test:unit && npm run test:integration",
    "lint": "eslint src --ext .ts,.tsx",
    "type-check": "tsc --noEmit",
    "prepare": "npm run build",
    "prepublishOnly": "npm run test:all && npm run lint && npm run type-check",
    "publish:public": "npm publish --access public",
    "publish:beta": "npm publish --tag beta"
  },
  "keywords": [
    "docrouter",
    "api",
    "sdk",
    "typescript",
    "document-processing",
    "ocr",
    "llm"
  ],
  "author": "Analytiq",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.13.2",
    "bson": "^7.1.1",
    "jsonwebtoken": "^9.0.3",
    "mongodb": "^7.0.0"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^25.0.10",
    "@typescript-eslint/eslint-plugin": "^8.53.1",
    "@typescript-eslint/parser": "^8.53.1",
    "eslint": "^9.39.2",
    "jest": "^30.2.0",
    "ts-jest": "^29.4.6",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "typescript": ">=4.9.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/analytiq/doc-router.git",
    "directory": "packages/typescript/sdk"
  },
  "bugs": {
    "url": "https://github.com/analytiq/doc-router/issues"
  },
  "homepage": "https://github.com/analytiq/doc-router#readme",
  "publishConfig": {
    "access": "public"
  }
}
