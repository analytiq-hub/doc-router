{
  "name": "@docrouter/mcp",
  "version": "1.0.0",
  "description": "TypeScript MCP server for DocRouter API",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "CLAUDE.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "start": "node dist/index.js",
    "start:dev": "tsx src/index.ts",
    "debug": "./mcp-inspector.sh",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts,.tsx",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "docrouter",
    "mcp",
    "model-context-protocol",
    "typescript",
    "document-processing",
    "ocr",
    "llm"
  ],
  "author": "Analytiq",
  "license": "MIT",
  "dependencies": {
    "@docrouter/sdk": "^1.0.0",
    "@modelcontextprotocol/sdk": "^1.25.3",
    "zod": "^4.3.6"
  },
  "overrides": {
    "qs": ">=6.14.1"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^25.0.10",
    "@typescript-eslint/eslint-plugin": "^8.53.1",
    "@typescript-eslint/parser": "^8.53.1",
    "eslint": "^9.39.2",
    "jest": "^30.2.0",
    "ts-jest": "^29.4.6",
    "tsup": "^8.5.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "bin": {
    "docrouter-mcp": "./dist/index.js"
  }
}
